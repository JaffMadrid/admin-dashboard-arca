{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/authentication/(.*)",
            "dest": "/authentication/$1"
          },
          {
            "src": "/dashboard/(.*)",
            "dest": "/dashboard/$1"
          },
          {
            "handle": "filesystem"
          },
          {
            "src": "/(.*)",
            "dest": "/index.html"
          }
    ]
}